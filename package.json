    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
